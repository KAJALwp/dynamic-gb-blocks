{
  "$schema": "https://schemas.wp.org/trunk/block.json",
  "name": "opry-plugin/insta-blog",
  "version": "0.1.0",
  "title": "Instagram Blog",
  "category": "opry-plugin",
  "icon": "instagram",
  "description": "Instagram Blog block.",
  "keywords": [
    "Instagram",
    "Dynamic"
  ],
  "supports": {
    "html": false
  },
  "textdomain": "opry-plugin",
  "attributes": {
    "heading": {
      "type": "string",
      "default": ""
    },
    "headingColor": {
      "type": "string",
      "default": ""
    },
    "btnText": {
      "type": "string",
      "default": ""
    },
    "accessToken": {
      "type": "string",
      "default": ""
    },
    "bgColor": {
      "type": "string",
      "default": ""
    },
    "bgImage": {
      "type": "object",
      "default": {
        "id": "",
        "url": "",
        "alt": "",
        "width": "",
        "height": ""
      }
    },
    "bgGradient": {
      "type": "string",
      "default": "trasparent"
    }
  },
  "editorScript": "file:./index.js",
  "editorStyle": "file:./index.css",
  "style": "file:./style-index.css",
  "viewScript": [
    "file:./view.js"
  ]
}